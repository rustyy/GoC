name = GoC
description = Panels Everywhere theme for GamesOnCampus.
version = VERSION
core = 7.x

features[] = logo
features[] = name
features[] = favicon
features[] = main_menu
features[] = secondary_menu

plugins[panels][layouts] = plugins/layouts
plugins[panels][styles] = plugins/styles

; We provide default page manager pages for our site template.
api[page_manager][pages_default][version] = 1    
api[page_manager][pages_default][path] = pages  

;include necessary 960gs styles
stylesheets[all][] = 960/reset.css
stylesheets[all][] = 960/960_24_col.css

;include style.css
stylesheets[all][] = css/style.css

regions[content] = Content

;settings[devgrid] = 0
;settings[devdiagnostic] = 0
;settings[devdebug] = 0